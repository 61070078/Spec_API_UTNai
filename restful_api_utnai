            newPass:
              type: string
              example: Rickiest_rick
            confirm:
              type: string
              example: Rickiest_rick
      responses:
        200:
          description: Password Changed!
          schema:
            properties:
              statusCode:
                type: integer
                example: 2000010
              statusText:
                type: string
                example: Password Changed!
  
  
